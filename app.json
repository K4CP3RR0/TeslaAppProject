{
  "expo": {
    "name": "TeslaCloneProject",
    "slug": "TeslaClone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.dev808.TeslaApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Tesla App ",
    "githubUrl": "https://github.com/K4CP3RR0/TeslaAppProject"
  }
}
